<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.gitt.kworks.mappers.KwsSysDataCtgryMapper">

	<select id="listAll" parameterType="java.util.Map" resultType="kwsSysDataCtgry">
		SELECT
			SYS_ID,
			DATA_CTGRY_ID
		FROM KWS_SYS_DATA_CTGRY
		WHERE
			SYS_ID = #{sysId}
	</select>
	

</mapper>