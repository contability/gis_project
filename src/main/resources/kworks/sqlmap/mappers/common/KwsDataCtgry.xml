<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.gitt.kworks.mappers.KwsDataCtgryMapper">

	<!-- 목록 검색 -->
	<select id="listAll" resultType="kwsDataCtgry">
		SELECT
			DATA_CTGRY_ID,
			DATA_CTGRY_NM
		FROM
			KWS_DATA_CTGRY
		WHERE
			USE_AT = 'Y'
	</select>

</mapper>