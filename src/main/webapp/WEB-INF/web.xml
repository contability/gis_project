<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://JAVA.SUN.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>kworks</display-name>
  <filter>
    <filter-name>encodingFilter</filter-name>
    <filter-class>org.springframework.web.filter.CharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>utf-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>encodingFilter</filter-name>
    <url-pattern>*.do</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>XSS</filter-name>
    <filter-class>kr.co.gitt.kworks.co.web.KworksCoXssFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>XSS</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>springSecurityFilterChain</filter-name>
    <filter-class>org.springframework.web.filter.DelegatingFilterProxy</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>springSecurityFilterChain</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <context-param>
    <param-name>spring.profiles.active</param-name>
    <param-value>yy_dev</param-value>
  </context-param>
  <context-param>
    <param-name>contextConfigLocation</param-name>
    <param-value>
		classpath*:kworks/spring/context-*.xml
		classpath*:projects/yy/spring/context-*.xml
	</param-value>
  </context-param>
  <listener>
    <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
  </listener>
  <listener>
    <listener-class>com.clipsoft.clipreport.server.service.listener.ReportSessionListener</listener-class>
  </listener>
  <servlet>
    <servlet-name>action</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <init-param>
      <param-name>contextConfigLocation</param-name>
      <param-value>/WEB-INF/config/kworks-*.xml</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>action</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <login-config>
    <auth-method>BASIC</auth-method>
  </login-config>
  <listener>
    <listener-class>kr.co.gitt.kworks.cmmn.session.SessionListner</listener-class>
  </listener>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <error-page>
    <exception-type>java.lang.Throwable</exception-type>
    <location>/common/error.jsp</location>
  </error-page>
  <error-page>
    <error-code>401</error-code>
    <location>/common/authenticationError.jsp</location>
  </error-page>
  <error-page>
    <error-code>404</error-code>
    <location>/common/error.jsp</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/common/error.jsp</location>
  </error-page>
  <servlet>
    <servlet-name>InitLegacy</servlet-name>
    <servlet-class>com.api.lo.legacy</servlet-class>
    <load-on-startup>100</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>SSOInLegacyManager</servlet-name>
    <servlet-class>com.api.SSOInLegacyManager</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SSOOutLegacyManager</servlet-name>
    <servlet-class>com.api.SSOOutLegacyManager</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SSOLegacyManager</servlet-name>
    <servlet-class>com.api.SSOLegacyManager</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SSOInLegacyManager</servlet-name>
    <url-pattern>/SSOInLegacyManager</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SSOOutLegacyManager</servlet-name>
    <url-pattern>/SSOOutLegacyManager</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SSOLegacyManager</servlet-name>
    <url-pattern>/SSOLegacyManager</url-pattern>
  </servlet-mapping>
  <env-entry>
    <env-entry-name>sso_url</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>108.5.2.80</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>sso_port</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>8001</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>ltnr_url</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>/sso/servlet/SSOListener</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>legc_id</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>gis_new</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>link_page</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>/ssoLoginAction.do</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>ses_name1</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>userId</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>ses_name2</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>userNam</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>ses_name3</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>deptCode</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>ses_name4</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value> </env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>ses_name5</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value> </env-entry-value>
  </env-entry>
</web-app>